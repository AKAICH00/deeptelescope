{
  "name": "ai-collab-swarm",
  "displayName": "AI Collaboration Swarm",
  "description": "Multi-agent AI orchestration MCP server with Opus Planner, Codex Coder, and Self-Correcting Swarm Review",
  "version": "1.0.0",
  "publisher": "antigravity",
  "bin": {
    "ai-collab-swarm": "./dist/mcp-server.js"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages",
    "Machine Learning",
    "Other"
  ],
  "keywords": [
    "ai",
    "claude",
    "code generation",
    "multi-agent",
    "swarm"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aiCollab.planTask",
        "title": "AI Collab: Plan Task with Opus"
      },
      {
        "command": "aiCollab.executeTask",
        "title": "AI Collab: Execute Full Pipeline"
      },
      {
        "command": "aiCollab.reviewCode",
        "title": "AI Collab: Review Code with Swarm"
      },
      {
        "command": "aiCollab.configure",
        "title": "AI Collab: Configure Settings"
      }
    ],
    "configuration": {
      "title": "AI Collaboration",
      "properties": {
        "aiCollab.anthropicApiKey": {
          "type": "string",
          "default": "",
          "description": "Anthropic API key for Claude models"
        },
        "aiCollab.hfToken": {
          "type": "string",
          "default": "",
          "description": "HuggingFace token for swarm models"
        },
        "aiCollab.swarmSize": {
          "type": "number",
          "default": 4,
          "description": "Number of agents in the review swarm"
        },
        "aiCollab.consensusThreshold": {
          "type": "number",
          "default": 0.6,
          "description": "Approval threshold for swarm consensus (0.0-1.0)"
        },
        "aiCollab.autoConfirm": {
          "type": "boolean",
          "default": false,
          "description": "Auto-confirm plan execution without prompting"
        }
      }
    },
    "menus": {
      "editor/context": [
        {
          "command": "aiCollab.reviewCode",
          "group": "aiCollab",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build:extension",
    "build": "tsc",
    "build:extension": "esbuild src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node --minify --sourcemap",
    "build:mcp": "esbuild src/mcp-server.ts --bundle --outfile=dist/mcp-server.js --format=cjs --platform=node --minify --sourcemap",
    "watch": "esbuild src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node --watch",
    "start": "ts-node src/index.ts",
    "mcp": "ts-node src/mcp-server.ts",
    "mcp:start": "node dist/mcp-server.js",
    "cli": "ts-node src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "package": "vsce package"
  },
  "dependencies": {
    "@google-cloud/vertexai": "^1.10.0",
    "@google/generative-ai": "^0.24.1",
    "@huggingface/inference": "^4.13.4",
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@qdrant/js-client-rest": "^1.16.2",
    "chalk": "^4.1.2",
    "dotenv": "^17.2.3",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "openai": "^6.10.0"
  },
  "mcp": {
    "name": "ai-collab",
    "version": "1.0.0",
    "description": "Multi-agent AI orchestration MCP server with swarm review, code search, and task planning",
    "server": {
      "command": "node",
      "args": [
        "dist/mcp-server.js"
      ],
      "env": {}
    },
    "tools": [
      {
        "name": "swarm_review",
        "description": "Multi-agent code review with self-correcting swarm intelligence",
        "inputSchema": {
          "type": "object",
          "properties": {
            "code": {
              "type": "string",
              "description": "Code to review"
            },
            "context": {
              "type": "string",
              "description": "Additional context about the code"
            },
            "swarmSize": {
              "type": "number",
              "description": "Number of agents in swarm (default: 4)"
            }
          },
          "required": [
            "code"
          ]
        }
      },
      {
        "name": "search_codebase",
        "description": "Semantic search across codebase using vector embeddings",
        "inputSchema": {
          "type": "object",
          "properties": {
            "query": {
              "type": "string",
              "description": "Search query"
            },
            "limit": {
              "type": "number",
              "description": "Maximum results to return (default: 5)"
            },
            "filter": {
              "type": "object",
              "description": "Optional metadata filters"
            }
          },
          "required": [
            "query"
          ]
        }
      },
      {
        "name": "plan_task",
        "description": "Create detailed implementation plan using Claude Opus",
        "inputSchema": {
          "type": "object",
          "properties": {
            "task": {
              "type": "string",
              "description": "Task description"
            },
            "context": {
              "type": "string",
              "description": "Additional context"
            }
          },
          "required": [
            "task"
          ]
        }
      }
    ]
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.85.0",
    "@types/ws": "^8.18.1",
    "@vscode/vsce": "^2.22.0",
    "esbuild": "^0.20.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.0.0",
    "vitest": "^1.6.1"
  }
}